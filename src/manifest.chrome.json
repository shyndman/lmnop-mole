{
	"manifest_version": 3,
	"name": "mole",
	"version": "0.11.11",
	"description": "Automatically extracts content from web pages.",
	"permissions": [
		"activeTab",
		"scripting",
		"storage",
		"alarms"
	],
	"host_permissions": [
		"http://*/*",
		"https://*/*"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	}
}
